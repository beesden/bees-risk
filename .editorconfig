[*]
charset=utf-8
end_of_line=crlf
insert_final_newline=false
indent_style=space
indent_size=4

[{.babelrc,.stylelintrc,.eslintrc,*.bowerrc,*.jsb3,*.jsb2,*.json}]
indent_style=tab
tab_width=4

[*.java]
indent_style=tab
tab_width=4

[{*.ddl,*.sql}]
indent_style=space
indent_size=2

[*.scss]
indent_style=space
indent_size=2

